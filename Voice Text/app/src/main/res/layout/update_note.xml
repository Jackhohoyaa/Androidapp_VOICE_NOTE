        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.941" />

    <TextView
        android:id="@+id/start2"
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:textColor="#FFFFFFFF"
        android:textColorHint="	#D3D3D3"
        android:textSize="16sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.202" />

    <ImageView
        android:id="@+id/options2"
        android:layout_width="36dp"
        android:layout_height="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.958"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.019"
        app:srcCompat="@mipmap/menu" />

    <TextView
        android:id="@+id/hint2"
        android:layout_width="198dp"
        android:layout_height="233dp"
        android:layout_marginBottom="56dp"
        android:background="#0A0E32"
        android:text="\n                 語音功能提示:\n\n                         空格\n                         刪除\n                         更新\n                       上一頁\n                       下一行\n                     標點符號\n                 刪除所有文字\n          長按麥克風切換語言"
        android:textColor="#D3D3D3"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/editText2"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextMultiLine2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/editTextTextMultiLine2"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout"
        app:layout_constraintVertical_bias="0.53" />

    <ImageView
        android:id="@+id/return_menu2"
        android:layout_width="34dp"
        android:layout_height="34dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018"
        app:srcCompat="@mipmap/back" />

    <EditText
        android:id="@+id/filename2"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#FF8400"
        android:ems="10"
        android:inputType="text"
        android:textColor="#FFFFFFFF"
        android:textColorHint="	#D3D3D3"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextMultiLine2"
        app:layout_constraintEnd_toStartOf="@+id/options2"
        app:layout_constraintStart_toEndOf="@+id/return_menu2"
        app:layout_constraintTop_toTopOf="@+id/textInputLayout2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/update"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:backgroundTint="@color/orange"
        android:text="update"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/Microphone2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="@+id/editTextTextMultiLine2"
        app:layout_constraintTop_toBottomOf="@+id/options2"
        app:layout_constraintVertical_bias="0.04" />

    <Button
        android:id="@+id/delete"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:backgroundTint="@color/orange"
        android:text="delete"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/Microphone2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.951"
        app:layout_constraintStart_toStartOf="@+id/editTextTextMultiLine2"
        app:layout_constraintTop_toBottomOf="@+id/update"
        app:layout_constraintVertical_bias="0.014" />

    <Button
        android:id="@+id/tips2"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:backgroundTint="@color/orange"
        android:text="tips"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/Microphone2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.951"
        app:layout_constraintStart_toStartOf="@+id/editTextTextMultiLine2"
        app:layout_constraintTop_toBottomOf="@+id/delete"
        app:layout_constraintVertical_bias="0.016" />

    <TextView
        android:id="@+id/id_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="468dp" />

</androidx.constraintlayout.widget.ConstraintLayout>